<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="thu.change">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/challenge_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/challenge_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GraphicActivity"
            android:label="@string/GraphicActivity"
            android:theme="@style/AppTheme.DarkActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".EntryActivity"
            android:label="@string/EntryActivity"
            android:theme="@style/AppTheme.DarkActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".CreateActivity"
            android:label="@string/CreateActivity"
            android:theme="@style/AppTheme.DarkActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".ChooseActivity"
            android:label="@string/ChooseActivity"
            android:theme="@style/AppTheme.DarkActionBar"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.DarkActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver
            android:name=".NotificationReceiver"
            android:enabled="true"
            android:exported="false" >
        </receiver>
    </application>

</manifest>